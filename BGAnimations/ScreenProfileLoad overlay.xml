<ActorFrame><children>
	<Layer Type="Quad"
		InitCommand="%function(self) Center(self);
		self:zoomto(SCREEN_CENTER_X*1.5,64);
		self:diffuse(0,0,0,0.625);
			self:cropright(1);
		end"
		OnCommand="decelerate,1;cropright,0;bounceend,0.5;glow,1,1,1,0.75;zoomy,0"
		/>
	<Layer Font="mentone/_24px" Text="All Profiles Loaded!"
		InitCommand="%function(self)
		Center(self);
		self:shadowlength(0)
		self:diffusealpha(0)
		end"
		OnCommand="sleep,0.75;linear,0.25;diffusealpha,1;sleep,0.125;accelerate,0.25;diffuse,0,0,0,1;decelerate,0.125;diffusealpha,0;"
		/>
</children></ActorFrame>